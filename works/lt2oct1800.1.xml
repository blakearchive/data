          <desc id="lt2oct1800.1.ltr.01" dbi="lt2oct1800.1.1.LT">
               <objtitle><title type="editorial">Mr Butts, Great Marlborough Street</title>,
                              <objid><objnumber code="A1">object 1</objnumber></objid></objtitle>
               <physdesc desclevel="brief">
                    <objsize>20.1 x 16.2 cm.</objsize>
                    <objnote>
                         <p>REPEAT NOTE FROM NUMBER OF LEAVES IN WORK INFO IN THIS PARAGRAPH</p>
